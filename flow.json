    {
        "id": "8527a99e14da1dde",
        "type": "function",
        "z": "4d83ee90e8919264",
        "name": "Take last values",
        "func": "// The message contains more than one sensor read.\n// We want one only, the last one\n\nvar payload = msg.payload;\nvar lastLocation = null;\nvar lastAcceleration = null;\n\n// Finding the last\npayload.payload.forEach(item => {\n    if (item.name === 'location') {\n        lastLocation = item;\n    } else if (item.name === 'totalacceleration') {\n        lastAcceleration = item;\n    }\n});\n\nvar output = {\n    latitude: lastLocation ? lastLocation.values.latitude : null,\n    longitude: lastLocation ? lastLocation.values.longitude : null,\n    xAcceleration: lastAcceleration ? lastAcceleration.values.x : null,\n    yAcceleration: lastAcceleration ? lastAcceleration.values.y : null,\n    zAcceleration: lastAcceleration ? lastAcceleration.values.z : null\n};\n\nmsg.payload = output;\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
